{
  "name": "asl-monorepo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "install:all": "pnpm install --recursive",
    "build:client": "pnpm --filter client run build",
    "start:server": "pnpm --filter server run start",
    "start": "pnpm run install:all && pnpm run build:client && pnpm run start:server"
  }
}
